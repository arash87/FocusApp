@model IEnumerable<FocusAppTest2.ServiceReference1.AdminModel>
@{
    ViewBag.Title = "Contact";
}
<div @*class="ui-content"*@>
    <div id="container" id="contact">
        <ul data-role="listview" data-inset="true" class="ui-listview ui-listview-inset ui-corner-all ui-shadow">
            @foreach (var item in Model)
            {
                <li class="ui-li ui-li-static ui-btn-up-c ui-corner-top">
                    <h3 class="ui-li-heading">@item.Fname @item.Lname</h3>
                    <p class="ui-li-desc">@item.Phone</p>
                    <p class="ui-li-desc">@item.Email</p>
                </li>
            }
        </ul>
    </div>
</div>
@section scripts{
    <script>
        $(function () {
            $('.contactBar').addClass('ui-btn-active')
                        .addClass('ui-state-persist');
        });
    </script>
}
